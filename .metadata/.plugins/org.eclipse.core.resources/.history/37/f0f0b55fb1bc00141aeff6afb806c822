	public myLesson() {
		jframe = new JFrame();
		jframe.setBounds(new Rectangle(0, 0, 200, 150));
		jframe.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		jframe.setVisible(true);
	}

	// class CGFrame extends JFrame {
